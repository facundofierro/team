{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "clean": {
      "dependsOn": [],
      "outputs": []
    },
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "dist/**", "build/**"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "prettier-fix": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    },
    "types": {
      "outputs": ["dist/**/*.d.ts"]
    }
  },
  "remoteCache": {
    "enabled": true
  }
}
